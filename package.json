    "dev": "cross-env NODE_ENV=development APP_TYPE=server nodemon --ignore data/*.json ./main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmeng93/Get-Free-SS.git"
  },
